{
  "extends": "airbnb",
  "plugins": [],
  "root": true,
  "rules": {
    "arrow-body-style": 0,
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "never"
    }],
    "curly": 0,
    "func-names": 0,
    "max-len": ["error", 80],
    "no-case-declarations": 0,
    "no-continue": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-restricted-syntax": 0,
    "no-undef": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "padded-blocks": 0,
    "prefer-rest-params": 0,
    "prefer-spread": 0,
    "prefer-template": 0,
    "strict": 0,
    "valid-typeof": 0
  }
}
